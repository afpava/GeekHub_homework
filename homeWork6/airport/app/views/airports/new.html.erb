<% if @airport.errors.any? %>
  <div id="error_explanation">
        <ul>
    <% @airport.errors.full_messages.each do |message| %>
      <li class="alert alert-danger"><%= message %></li>
    <% end %>
    </ul>
  </div>
<% end %>

  <h3> Create Airport:</h3>
<%= render partial: 'shared/form', :locals => { parent: @city, pl_text: "Airport", children: @city.airports.build } %>

<%= link_to 'Back', city_path(@city) , :class=>'btn btn-outline-secondary' %>
